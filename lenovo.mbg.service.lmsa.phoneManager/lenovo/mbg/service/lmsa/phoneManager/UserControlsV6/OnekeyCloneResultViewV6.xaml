<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="lenovo.mbg.service.lmsa.phoneManager.UserControlsV6.OnekeyCloneResultViewV6" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:lang="clr-namespace:lenovo.mbg.service.framework.lang;assembly=lenovo.mbg.service.framework.lang" xmlns:gc6="clr-namespace:lenovo.themes.generic.ControlsV6;assembly=lenovo.themes.generic" xmlns:local="clr-namespace:lenovo.mbg.service.lmsa.phoneManager.UserControlsV6">
  <FrameworkElement.Resources>
    <ResourceDictionary>
      <Style x:Key="ListBoxItemStyle1" TargetType="{x:Type ListBoxItem}">
        <Setter Property="Background" Value="#00FFFFFF" />
        <Setter Property="HorizontalContentAlignment" Value="{Binding HorizontalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Padding" Value="2,0,0,0" />
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="{x:Type ListBoxItem}">
              <Grid>
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="*" />
                  <ColumnDefinition Width="auto" />
                  <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                  <RowDefinition />
                  <RowDefinition Height="38" />
                  <RowDefinition />
                </Grid.RowDefinitions>
                <Line Grid.Column="0" Grid.ColumnSpan="3" Stroke="#D4DCE3" StrokeThickness="1" X1="0" Y1="1.5" X2="1" Y2="1.5" Stretch="Uniform" Visibility="{Binding LineVisibility}" />
                <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center">
                  <ToggleButton Name="_closePanelButton" Visibility="Collapsed" VerticalAlignment="Center" Margin="0 0 0 0" ClickMode="Press" IsChecked="{Binding IsFailedItemsPanelClosed, Mode=TwoWay}">
                    <Control.Template>
                      <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Border>
                          <Image Name="ExpanderIcon" Width="14" Height="14" Source="{DynamicResource v6_icon_left}" RenderTransformOrigin="0.5,0.5" />
                        </Border>
                        <ControlTemplate.Triggers>
                          <Trigger Property="ToggleButton.IsChecked" Value="true">
                            <Setter TargetName="ExpanderIcon" Property="RenderTransform">
                              <Setter.Value>
                                <RotateTransform Angle="270" />
                              </Setter.Value>
                            </Setter>
                          </Trigger>
                        </ControlTemplate.Triggers>
                      </ControlTemplate>
                    </Control.Template>
                  </ToggleButton>
                  <lang:LangTextBlock Name="resname" Margin="3 0 0 0" Style="{StaticResource v6_comm_text_style_9}" LangText="{Binding ResName}" LangKey="{Binding ResName}" />
                  <TextBlock Name="resnamecount" Margin="3 0 0 0" Style="{StaticResource v6_comm_text_style_9}" Text="{Binding ResNameCount}" />
                </StackPanel>
                <StackPanel Name="completed" Margin="0 0 10 0" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Orientation="Horizontal" Visibility="Collapsed">
                  <lang:LangTextBlock LangKey="K0638" VerticalAlignment="Center" Style="{StaticResource v6_comm_text_style_9}" />
                  <Image Margin="8 0 0 0" Width="20" Height="20" Stretch="Fill" Source="{StaticResource v6_icon_confirm}" />
                </StackPanel>
                <StackPanel Name="hasfailed" Margin="0 0 10 0" Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" Orientation="Horizontal" Visibility="Collapsed">
                  <lang:LangTextBlock LangKey="K0337" VerticalAlignment="Center" Style="{StaticResource v6_comm_text_style_9}" />
                  <TextBlock Text=":" Style="{StaticResource v6_comm_text_style_9}" />
                  <TextBlock Margin="4 0 0 0" Style="{StaticResource v6_comm_text_style_7}" Text="{Binding Failed}" />
                </StackPanel>
                <StackPanel Name="_failedList" Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="2" Visibility="Collapsed">
                  <TextBlock LineHeight="23" Style="{StaticResource v6_comm_text_style_7}" Visibility="{Binding MissingGroupVisibility}">
                    <Run Text="{Binding MissingCount}" /> <Run Text="{Binding MissingRowTital}" /></TextBlock>
                  <ItemsControl BorderThickness="0" ScrollViewer.VerticalScrollBarVisibility="Disabled" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ItemsSource="{Binding FailedItemList}">
                    <ItemsControl.ItemTemplate>
                      <DataTemplate>
                        <Border BorderThickness="0" BorderBrush="#00FFFFFF">
                          <Grid>
                            <Grid.ColumnDefinitions>
                              <ColumnDefinition Width="*" />
                              <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>
                            <lang:LangTextBlock LineHeight="23" Style="{StaticResource v6_comm_text_style_7}" LangText="{Binding Path}" LangKey="{Binding Path}" />
                            <StackPanel Margin="0 0 10 0" Grid.Column="1" VerticalAlignment="Center" Orientation="Horizontal">
                              <lang:LangTextBlock LangKey="K0337" VerticalAlignment="Center" />
                              <Image Margin="8 0 0 0" Width="20" Height="20" Stretch="Fill" Source="{StaticResource v6_icon_error}" />
                            </StackPanel>
                          </Grid>
                        </Border>
                      </DataTemplate>
                    </ItemsControl.ItemTemplate>
                  </ItemsControl>
                </StackPanel>
              </Grid>
              <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                  <Setter Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}" Property="Foreground" />
                </Trigger>
                <DataTrigger Value="false" Binding="{Binding IsComplete}">
                  <Setter TargetName="_closePanelButton" Property="Visibility" Value="Visible" />
                  <Setter TargetName="hasfailed" Property="Visibility" Value="Visible" />
                </DataTrigger>
                <DataTrigger Value="true" Binding="{Binding IsComplete}">
                  <Setter TargetName="_closePanelButton" Property="Visibility" Value="Collapsed" />
                  <Setter TargetName="completed" Property="Visibility" Value="Visible" />
                </DataTrigger>
                <DataTrigger Value="true" Binding="{Binding IsChecked, ElementName=_closePanelButton}">
                  <Setter TargetName="_failedList" Property="Visibility" Value="Visible" />
                  <Setter TargetName="resname" Property="TextBlock.FontWeight" Value="Medium" />
                  <Setter TargetName="resnamecount" Property="TextBlock.FontWeight" Value="Medium" />
                </DataTrigger>
              </ControlTemplate.Triggers>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Style>
      <Style x:Key="ListBoxStyle1" TargetType="{x:Type ListBox}">
        <Setter Property="Background" Value="#00FFFFFF" />
        <Setter Property="ScrollViewer.CanContentScroll" Value="False" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
        <Setter Property="ScrollViewer.PanningMode" Value="Both" />
        <Setter Property="Stylus.IsFlicksEnabled" Value="False" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="ItemContainerStyle" Value="{StaticResource ListBoxItemStyle1}" />
        <Setter Property="Template">
          <Setter.Value>
            <ControlTemplate TargetType="{x:Type ListBox}">
              <Border Name="Bd" BorderBrush="#FFF6F6F6" BorderThickness="{TemplateBinding Control.BorderThickness}" Background="{TemplateBinding Control.Background}" Padding="1" SnapsToDevicePixels="True">
                <ScrollViewer Focusable="False" Padding="{TemplateBinding Control.Padding}" HorizontalScrollBarVisibility="Disabled">
                  <ItemsPresenter SnapsToDevicePixels="{TemplateBinding UIElement.SnapsToDevicePixels}" />
                </ScrollViewer>
              </Border>
              <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                  <Setter TargetName="Bd" Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}" Property="Border.Background" />
                </Trigger>
                <MultiTrigger>
                  <MultiTrigger.Conditions>
                    <Condition Property="ItemsControl.IsGrouping" Value="True" />
                    <Condition Property="VirtualizingPanel.IsVirtualizingWhenGrouping" Value="False" />
                  </MultiTrigger.Conditions>
                  <Setter Property="ScrollViewer.CanContentScroll" Value="False" />
                </MultiTrigger>
              </ControlTemplate.Triggers>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Style>
    </ResourceDictionary>
  </FrameworkElement.Resources>
  <Border Padding="0 41" HorizontalAlignment="Center">
    <Grid>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="440" />
        <ColumnDefinition Width="auto" />
      </Grid.ColumnDefinitions>
      <Grid.RowDefinitions>
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="auto" />
        <RowDefinition Height="*" />
        <RowDefinition Height="auto" />
      </Grid.RowDefinitions>
      <lang:LangTextBlock Grid.Row="0" Style="{StaticResource v6_comm_text_style_8}">
        <lang:LangRun LangKey="K1386" /> <Run Foreground="#005D7F" Text="{Binding TransferViewModel.TargetModelName}" /> <lang:LangRun LangKey="K1387" /></lang:LangTextBlock>
      <StackPanel Grid.Row="1" Orientation="Horizontal" Margin="0 9 0 0">
        <lang:LangTextBlock Style="{StaticResource v6_comm_text_style_9}">
          <lang:LangRun LangKey="K0634" /> <lang:LangRun Foreground="#01D9B1" LangKey="{Binding TransferViewModel.TransferSuccessTotal}" /></lang:LangTextBlock>
        <lang:LangTextBlock Style="{StaticResource v6_comm_text_style_9}" Margin="27 0 0 0">
          <lang:LangRun LangKey="K0337" /> <Run Text=":" /> <lang:LangRun Foreground="#FF4042" LangKey="{Binding TransferViewModel.TransferFailedTotal}" /></lang:LangTextBlock>
      </StackPanel>
      <ProgressBar Grid.Row="2" Maximum="100" Style="{StaticResource v6_comm_progressbar_style}" Margin="0 40 0 0" gc6:ControlAttachPropertyV6.CommVisibility="Collapsed" gc6:ControlAttachPropertyV6.MouseOverBackground="{StaticResource v6_comm_brush_ltrb2_style_1}" gc6:ControlAttachPropertyV6.MouseOverForeground="{StaticResource v6_comm_brush_ltrb2_style_1}" Height="12" Value="0" Minimum="0" />
      <Grid Row="3">
        <Grid.RowDefinitions>
          <RowDefinition Height="*" />
          <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <ListBox Style="{StaticResource ListBoxStyle1}" Margin="0 20 0 0" ItemsSource="{Binding TransferViewModel.OperResultArr}" />
        <StackPanel Grid.Row="1" HorizontalAlignment="Left" Orientation="Horizontal" VerticalAlignment="Center" Margin="0 12 0 0">
          <Button Style="{StaticResource v6_comm_base_button}" gc6:ControlAttachPropertyV6.CornerRadius="19" Background="#FFFFFF" BorderThickness="1" BorderBrush="#D4DCE3" Foreground="#005D7F" Content="K0636" MinWidth="115" Height="38" Visibility="{Binding TransferViewModel.RetryButtonVisibility}" Command="{Binding RetryCommand}" />
          <Button Margin="15 0 0 0" Style="{StaticResource v6_comm_base_button}" gc6:ControlAttachPropertyV6.CornerRadius="19" Background="#005D7F" BorderThickness="0" Foreground="#FFFFFF" Content="K0327" MinWidth="115" Height="38" Command="{Binding OKCommand}" />
        </StackPanel>
      </Grid>
      <Grid Column="1" RowSpan="5" Width="163" Height="351" Margin="81 20 70 0" VerticalAlignment="Top">
        <Image Source="{DynamicResource v6_Mydevice_icon}" />
        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Style="{StaticResource v6_comm_text_style_1}">
          <Run Text="SN:" /> <Run Text="{Binding TransferViewModel.SelectedTargetDevice.SN}" /></TextBlock>
      </Grid>
    </Grid>
  </Border>
</UserControl>