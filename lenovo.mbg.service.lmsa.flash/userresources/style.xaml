              <ResourceDictionary>
                <SolidColorBrush x:Key="deletePersonalDataBackBround">#ffffff</SolidColorBrush>
              </ResourceDictionary>
            </FrameworkElement.Resources>
            <Grid Width="320" Height="auto" Background="#00FFFFFF" Margin="20">
              <Border Background="#FFFFFF" Margin="0 7 0 0" CornerRadius="5">
                <UIElement.Effect>
                  <DropShadowEffect Color="Gray" BlurRadius="20" />
                </UIElement.Effect>
                <StackPanel Margin="11">
                  <Border>
                    <lang:LangTextBlock Margin="0 3 0 10" LineHeight="10" FontSize="14" FontWeight="Medium" HorizontalAlignment="Left" TextWrapping="Wrap" LangText="K1253" LangKey="K1253" Foreground="#494949" />
                  </Border>
                  <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <lang:LangButton Width="140" Height="auto" Foreground="#FF808080" Background="#FFFFFFFF" Command="{Binding PopCancelClick}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}">
                      <Control.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                          <Border BorderThickness="1" BorderBrush="#FF808080" Background="#FFFFFFFF" CornerRadius="8" Padding="10 5">
                            <lang:LangTextBlock HorizontalAlignment="Center" VerticalAlignment="Center" LangText="K1254" LangKey="K1254" TextWrapping="Wrap" ToolTipService.ToolTip="{Binding Path=Text, RelativeSource={RelativeSource Mode=Self}}" />
                          </Border>
                        </ControlTemplate>
                      </Control.Template>
                    </lang:LangButton>
                    <lang:LangButton Margin="10 0 0 0" Width="140" Height="auto" Foreground="#FFFFFFFF" Background="#43B5E2" Command="{Binding PopOkClick}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}">
                      <Control.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                          <Border BorderThickness="1" Background="#43B5E2" BorderBrush="#43B5E2" CornerRadius="8" Padding="10 5">
                            <lang:LangTextBlock HorizontalAlignment="Center" VerticalAlignment="Center" LangText="K1255" LangKey="K1255" TextWrapping="Wrap" ToolTipService.ToolTip="{Binding Path=Text, RelativeSource={RelativeSource Mode=Self}}" />
                          </Border>
                        </ControlTemplate>
                      </Control.Template>
                    </lang:LangButton>
                  </StackPanel>
                </StackPanel>
              </Border>
              <Polygon Margin="20 0 10 0" VerticalAlignment="Top" Fill="#FFFFFFFF" Points="0,7 14,7 7,0" Stroke="#00FFFFFF" StrokeThickness="0" />
            </Grid>
          </genericControls:PopupEx>
          <Grid Row="1">
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="18" />
              <ColumnDefinition Width="158*" />
              <ColumnDefinition Width="63*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
              <RowDefinition Height="43" />
              <RowDefinition MinHeight="120" Height="*" />
              <RowDefinition Height="30" />
              <RowDefinition Height="10" />
              <RowDefinition Height="23" />
            </Grid.RowDefinitions>
            <Border Grid.Column="1" Grid.Row="0">
              <Grid VerticalAlignment="Center">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="auto" />
                  <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Border Margin="0 0 8 0" Visibility="{Binding WarnIconVisibility}">
                  <Image Width="24" Height="24" Source="{StaticResource rescue_warnicon}" />
                </Border>
                <Border Grid.Column="1">
                  <lang:LangTextBlock LineHeight="35" TextWrapping="Wrap" VerticalAlignment="Center" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Disabled" FontSize="16" Foreground="#545454" FontWeight="SemiBold" LangText="{Binding Title}" LangKey="{Binding Title}" />
