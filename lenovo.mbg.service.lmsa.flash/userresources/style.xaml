              </Border>
              <Border Grid.Column="1">
                <lang:LangTextBlock LangText="Checking condition..." LangKey="K0104" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="9pt" Foreground="#E28400" />
              </Border>
            </Grid>
            <Border Grid.Column="2" Grid.Row="0" Grid.RowSpan="4">
              <Image Stretch="None" Source="{Binding ContentIconImageSource}" />
            </Border>
          </Grid>
        </Grid>
      </ControlTemplate>
      <ControlTemplate x:Key="rescue_defaultDeviceOperationAutoOKWindowTemplateEx" TargetType="{x:Type UserControl}">
