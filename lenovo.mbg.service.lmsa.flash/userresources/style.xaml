    <ResourceDictionary>
      <ControlTemplate x:Key="rescue_defaultDeviceOperationAutoOKWindowTemplate" TargetType="{x:Type UserControl}">
        <Grid Background="#FFFFFF" Width="500" Height="350">
          <Grid.RowDefinitions>
            <RowDefinition Height="37" />
            <RowDefinition Height="*" />
          </Grid.RowDefinitions>
          <Border Grid.Row="0">
            <Button Name="closeButton" Width="14" Height="14" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 5 5 0">
              <Control.Template>
                <ControlTemplate TargetType="{x:Type Button}">
                  <Grid Name="root" Background="{Binding CancelButtonBackground}">
                    <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" />
                    <UIElement.InputBindings>
                      <MouseBinding Gesture="LeftClick" Command="{Binding CancelButtonClickCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}" />
                    </UIElement.InputBindings>
                  </Grid>
                  <ControlTemplate.Triggers>
                    <Trigger Property="UIElement.IsMouseOver" Value="True">
                      <Setter TargetName="root" Property="Panel.Background" Value="{Binding CancelButtonMouseOverBackground}" />
                    </Trigger>
                    <Trigger Property="UIElement.IsEnabled" Value="False">
                      <Setter TargetName="root" Property="Panel.Background" Value="{Binding CancelButtonDisabledBackground}" />
                    </Trigger>
                  </ControlTemplate.Triggers>
                </ControlTemplate>
              </Control.Template>
            </Button>
          </Border>
          <genericControls:PopupEx Name="pop" Placement="Bottom" Topmost="False" AllowsTransparency="True" HorizontalOffset="-20" VerticalOffset="8" UseLayoutRounding="True" StaysOpen="True" IsOpen="False" PlacementTarget="{Binding ElementName=closeButton}">
