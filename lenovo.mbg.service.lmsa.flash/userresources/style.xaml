              <ColumnDefinition Width="30" />
              <ColumnDefinition Width="150*" />
              <ColumnDefinition Width="63*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
              <RowDefinition Height="1" />
              <RowDefinition MinHeight="110" Height="*" />
              <RowDefinition Height="20" />
              <RowDefinition Height="30" />
              <RowDefinition Height="20" />
            </Grid.RowDefinitions>
            <Border Grid.Column="0" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0 4 0 0">
              <Image Stretch="None" Source="{StaticResource {ComponentResourceKey TypeInTargetAssembly=generic:ComponentResources, ResourceId=drawingImage_warning}}" />
            </Border>
            <Border Grid.Column="1" Grid.Row="0">
              <lang:LangTextBlock TextWrapping="Wrap" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Disabled" Foreground="#a4a5a5" FontSize="12pt" LangText="{Binding Title}" LangKey="{Binding Title}" />
