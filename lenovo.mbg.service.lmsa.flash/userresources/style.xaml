          <Setter.Value>
            <ControlTemplate TargetType="{x:Type TextBox}">
              <Border Name="Bd" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" Background="{TemplateBinding Control.Background}" Padding="{TemplateBinding Control.Padding}" CornerRadius="6">
                <Grid>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                  </Grid.ColumnDefinitions>
                  <ScrollViewer Name="PART_ContentHost" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden" Focusable="False" VerticalAlignment="Center" />
                  <TextBlock Name="def" Text="{TemplateBinding FrameworkElement.Tag}" VerticalAlignment="Center" Visibility="Collapsed" Margin="8 0 0 0" FontWeight="{TemplateBinding Control.FontWeight}" Opacity="0.7" />
                  <Button Name="PART_Empty" Grid.Column="1" Style="{StaticResource EmptyBtnStyle}" Visibility="Collapsed" Margin="0 0 4 0" />
                  <ToggleButton Name="PART_Toggle" Grid.Column="2" Width="20" Height="20">
                    <Control.Template>
                      <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Border Background="#00FFFFFF">
                          <Path Name="Arrow" Width="20" Height="20" Fill="#43B5E2" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Data="M4,7 L16,7 L10,15" />
                        </Border>
                        <ControlTemplate.Triggers>
                          <Trigger Property="ToggleButton.IsChecked" Value="true">
                            <Setter TargetName="Arrow" Property="RenderTransform">
                              <Setter.Value>
                                <RotateTransform Angle="180" />
                              </Setter.Value>
                            </Setter>
                          </Trigger>
                        </ControlTemplate.Triggers>
                      </ControlTemplate>
                    </Control.Template>
                  </ToggleButton>
                </Grid>
              </Border>
              <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                  <Setter Property="Background" Value="#FFEFF2F6" />
                </Trigger>
                <MultiTrigger>
                  <MultiTrigger.Conditions>
                    <Condition Property="Text" Value="" />
                    <Condition Property="UIElement.IsFocused" Value="False" />
                  </MultiTrigger.Conditions>
                  <Setter TargetName="def" Property="Visibility" Value="Visible" />
                </MultiTrigger>
                <MultiTrigger>
                  <MultiTrigger.Conditions>
                    <Condition Property="Text" Value="" />
                    <Condition Property="TextBoxBase.IsReadOnly" Value="True" />
                  </MultiTrigger.Conditions>
                  <Setter TargetName="def" Property="Visibility" Value="Visible" />
                </MultiTrigger>
              </ControlTemplate.Triggers>
            </ControlTemplate>
          </Setter.Value>
        </Setter>
      </Style>
