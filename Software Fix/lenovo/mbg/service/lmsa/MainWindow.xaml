              <Setter Property="ListBoxItem.IsSelected" Value="{Binding IsSelected, Mode=TwoWay}" />
              <Setter Property="Template">
                <Setter.Value>
                  <ControlTemplate TargetType="{x:Type ListViewItem}">
                    <Border Name="bd" Padding="7 5" BorderBrush="#D4DCE3" BorderThickness="0 0 0 1">
                      <TextBlock Foreground="{TemplateBinding Control.Foreground}" Text="{Binding Text}" />
                    </Border>
                  </ControlTemplate>
                </Setter.Value>
              </Setter>
            </Style>
          </ItemsControl.ItemContainerStyle>
        </ListView>
      </Border>
