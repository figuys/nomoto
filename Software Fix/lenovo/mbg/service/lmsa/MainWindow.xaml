    <Button Name="couponborder" Grid.Row="2" Margin="9 0 0 32" VerticalAlignment="Bottom" HorizontalAlignment="Left" Visibility="{Binding MiddleBannerVisibility}">
      <FrameworkElement.Style>
        <Style TargetType="{x:Type Button}">
          <Setter Property="Template">
            <Setter.Value>
              <ControlTemplate TargetType="{x:Type Button}">
                <Border CornerRadius="10">
                  <Border.Background>
                    <ImageBrush ImageSource="{StaticResource v6_Couponcode_smallbg}" />
                  </Border.Background>
                  <Grid Name="coupongrid" MinWidth="183" MinHeight="91" MaxWidth="300">
                    <Border Name="Bd" Background="#7C000000" Visibility="Collapsed" CornerRadius="10" Panel.ZIndex="99999">
                      <Button Height="32" MinWidth="128" VerticalAlignment="Center" HorizontalAlignment="Center">
                        <FrameworkElement.Style>
                          <Style TargetType="{x:Type Button}">
                            <Setter Property="FrameworkElement.Cursor" Value="Hand" />
                            <Setter Property="Foreground" Value="#6EC2ED" />
                            <Setter Property="Background" Value="#00FFFFFF" />
                            <Setter Property="BorderThickness" Value="1" />
                            <Setter Property="BorderBrush" Value="{DynamicResource v6_comm_black_color}" />
                            <Setter Property="FontSize" Value="14" />
                            <Setter Property="VerticalContentAlignment" Value="Center" />
                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                            <Setter Property="FontWeight" Value="SemiBold" />
                            <Setter Property="Padding" Value="19 0" />
                            <Setter Property="Template">
                              <Setter.Value>
                                <ControlTemplate TargetType="{x:Type Button}">
                                  <Border Name="NBd" BorderBrush="#6EC2ED" CornerRadius="2" Background="{TemplateBinding Control.Background}" BorderThickness="2" Padding="{TemplateBinding Control.Padding}" SnapsToDevicePixels="True">
                                    <i:Interaction.Triggers>
                                      <i:EventTrigger i:EventTrigger.EventName="MouseLeftButtonDown">
                                        <i:InvokeCommandAction i:InvokeCommandAction.CommandParameter="middle_banner_click" i:InvokeCommandAction.Command="{Binding BannerClickCommand}" />
                                      </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                                      <lang:LangTextBlock Name="content" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="{TemplateBinding Control.Foreground}" FontWeight="{TemplateBinding Control.FontWeight}" TextWrapping="Wrap" FontSize="{TemplateBinding Control.FontSize}">
                                        <lang:LangRun LangKey="K1591" /> <lang:LangRun LangKey="&gt;&gt;" /></lang:LangTextBlock>
                                    </Grid>
                                  </Border>
                                  <ControlTemplate.Triggers>
                                    <Trigger Property="UIElement.IsMouseOver" Value="True">
                                      <Setter TargetName="NBd" Property="Border.Background" Value="#005D7F" />
                                      <Setter TargetName="NBd" Property="Border.BorderThickness" Value="0" />
                                      <Setter Property="Foreground" Value="#FFFFFF" />
                                    </Trigger>
                                  </ControlTemplate.Triggers>
                                </ControlTemplate>
                              </Setter.Value>
                            </Setter>
                          </Style>
                        </FrameworkElement.Style>
                      </Button>
                    </Border>
                    <Button Name="couponclose" Margin="0 12 12 0" Style="{StaticResource v6_comm_img_button}" Panel.ZIndex="999" HorizontalAlignment="Right" VerticalAlignment="Top" Width="12" Height="13" gc6:ControlAttachPropertyV6.ImageSource="{StaticResource v6_icon_close}" CommandParameter="middle_banner_close" Command="{Binding BannerClickCommand}" />
                    <StackPanel Margin="12 5 12 0">
                      <WrapPanel Margin="45 0 20 0">
                        <lang:LangTextBlock Style="{StaticResource v6_comm_text_style_10}" LangKey="K0230" FontWeight="Bold" VerticalAlignment="Bottom" FontSize="12" />
                        <TextBlock Text="*" Foreground="#6EC2ED" FontSize="14" FontWeight="SemiBold" Margin="3 12 0 -3">
                          <UIElement.Effect>
                            <DropShadowEffect BlurRadius="0" ShadowDepth="1" Color="#FFFFFF" />
                          </UIElement.Effect>
                        </TextBlock>
                        <TextBlock Foreground="#6EC2ED" FontSize="25" FontWeight="Bold" Margin="3 3 3 -3" Text="{Binding Discount}">
                          <UIElement.Effect>
                            <DropShadowEffect BlurRadius="0" ShadowDepth="1" Color="#FFFFFF" />
                          </UIElement.Effect>
                        </TextBlock>
                        <lang:LangTextBlock Style="{StaticResource v6_comm_text_style_10}" LangKey="K1609" FontWeight="Bold" FontSize="13" VerticalAlignment="Bottom" />
                      </WrapPanel>
                      <lang:LangTextBlock Grid.Column="1" Style="{StaticResource v6_comm_text_style_6}" Foreground="#6EC2ED" FontSize="13" Margin="45 8 0 0" LangKey="{Binding MotoCare}" />
                    </StackPanel>
                  </Grid>
                </Border>
                <ControlTemplate.Triggers>
                  <Trigger SourceName="coupongrid" Property="UIElement.IsMouseOver" Value="True">
                    <Setter TargetName="Bd" Property="Visibility" Value="Visible" />
                  </Trigger>
                  <Trigger SourceName="couponclose" Property="UIElement.IsMouseOver" Value="True">
                    <Setter TargetName="Bd" Property="Visibility" Value="Collapsed" />
                  </Trigger>
                </ControlTemplate.Triggers>
              </ControlTemplate>
            </Setter.Value>
          </Setter>
        </Style>
      </FrameworkElement.Style>
    </Button>
    <Border Name="mutiltutorials" Padding="10 22 14 22" Margin="0 0 0 0" BorderThickness="1 0 0 0" BorderBrush="#00FFFFFF" Visibility="Collapsed" Grid.Column="1" Grid.RowSpan="3" Background="#FFFFFF" Width="458">
