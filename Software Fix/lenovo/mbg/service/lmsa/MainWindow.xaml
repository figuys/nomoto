    <Image Name="imgLoading" Grid.RowSpan="4" Visibility="Collapsed" Height="100" Width="100" Source="/Software Fix;component/ResourceV6/main_loading.png" Panel.ZIndex="1" RenderTransformOrigin="0.5 0.5">
      <UIElement.RenderTransform>
        <RotateTransform x:Name="imgLoadingAnimatedRotateTrans" Angle="0" />
      </UIElement.RenderTransform>
      <FrameworkElement.Triggers>
        <EventTrigger RoutedEvent="Loaded">
          <BeginStoryboard>
            <Storyboard>
              <DoubleAnimation Storyboard.TargetName="imgLoadingAnimatedRotateTrans" Storyboard.TargetProperty="Angle" From="0" To="360" Duration="0:0:2" RepeatBehavior="Forever" />
            </Storyboard>
          </BeginStoryboard>
        </EventTrigger>
      </FrameworkElement.Triggers>
    </Image>
    <Border Name="MaskGrid" Grid.RowSpan="3" Background="#7C000000" Visibility="Collapsed">
      <ContentPresenter />
    </Border>
    <Border Name="banner" Grid.Row="3" Visibility="{Binding BannerVisibility}">
      <Grid Width="{Binding Width, ElementName=mainWindow}">
        <Grid HorizontalAlignment="Center">
          <i:Interaction.Triggers>
            <i:EventTrigger i:EventTrigger.EventName="MouseLeftButtonDown">
              <i:InvokeCommandAction i:InvokeCommandAction.CommandParameter="banner_click" i:InvokeCommandAction.Command="{Binding BannerClickCommand}" />
            </i:EventTrigger>
          </i:Interaction.Triggers>
          <ContentPresenter />
          <Image Height="72" Stretch="Fill" Cursor="Hand" Source="{Binding BannerImage}" />
          <Button Foreground="#FFFFFF" FontSize="16" FontWeight="SemiBold" CommandParameter="banner_close" Command="{Binding BannerClickCommand}">
            <Control.Template>
              <ControlTemplate TargetType="{x:Type Button}">
                <Border Padding="12 5" Background="#20FFFFFF" HorizontalAlignment="Right" CornerRadius="0 0 0 10" VerticalAlignment="Top">
                  <TextBlock Text="Close" />
                </Border>
              </ControlTemplate>
            </Control.Template>
          </Button>
        </Grid>
      </Grid>
    </Border>
    <Border Cursor="Hand" Grid.Row="2" Width="200" Height="32" VerticalAlignment="Bottom" HorizontalAlignment="Center" Visibility="{Binding SmallBannerVisibility}">
      <Image Source="{Binding BannerImage}" />
      <i:Interaction.Triggers>
        <i:EventTrigger i:EventTrigger.EventName="MouseLeftButtonDown">
          <i:InvokeCommandAction i:InvokeCommandAction.CommandParameter="small_banner_click" i:InvokeCommandAction.Command="{Binding BannerClickCommand}" />
        </i:EventTrigger>
      </i:Interaction.Triggers>
