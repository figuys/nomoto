    </Grid>
    <Button Name="feedback" Grid.Row="2" Content="K0284" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0 0 10 6" Style="{DynamicResource V6_FeedbacBtnStyle}" IsEnabled="{Binding IsUiWorkEnabe}" Click="OnBtnFeedback" />
    <Grid Name="DownLoadCenterHere" Row="1" RowSpan="2" Width="420" HorizontalAlignment="Right" Visibility="Collapsed">
