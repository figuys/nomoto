      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Button}">
          <Grid Margin="8" Height="{TemplateBinding FrameworkElement.Height}">
            <Border CornerRadius="18" Background="{TemplateBinding Control.Background}" BorderThickness="{TemplateBinding Control.BorderThickness}" Padding="{TemplateBinding Control.Padding}" SnapsToDevicePixels="True">
