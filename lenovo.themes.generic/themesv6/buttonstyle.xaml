                  <GradientStop Color="#8976E1" Offset="0.5" />
                  <GradientStop Color="#3876D6" Offset="1" />
                </LinearGradientBrush>
              </Border.BorderBrush>
              <UIElement.Effect>
                <DropShadowEffect Color="#56054A" BlurRadius="12" Opacity="0.27" ShadowDepth="1" />
              </UIElement.Effect>
              <lang:LangTextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="{TemplateBinding Control.Foreground}" FontWeight="{TemplateBinding Control.FontWeight}" TextWrapping="Wrap" FontSize="{Binding FontSize, RelativeSource={RelativeSource AncestorType=Button, Mode=FindAncestor}}" LangKey="{Binding Content, RelativeSource={RelativeSource AncestorType=Button, Mode=FindAncestor}}" />
