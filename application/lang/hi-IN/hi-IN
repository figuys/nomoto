	<sys:String x:Key="K1200"  xml:space="preserve">डिस्कनेक्ट</sys:String>
	<sys:String x:Key="K1201"  xml:space="preserve">पावर ऑफ </sys:String>
	<sys:String x:Key="K1202"  xml:space="preserve">डिवाइस का सही से पता नहीं चल पाया है.</sys:String>
