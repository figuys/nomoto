
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>lenovo.mbg.service.lmsa.hostproxy</AssemblyName>
    <GenerateAssemblyInfo>False</GenerateAssemblyInfo>
    <UseWPF>True</UseWPF>
    <TargetFramework>net472</TargetFramework>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>11.0</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup>
    <RootNamespace>lenovo.mbg.service.lmsa.hostproxy</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\framework.services\framework.services.csproj" />
    <ProjectReference Include="..\framework.smartbase\framework.smartbase.csproj" />
    <ProjectReference Include="..\framework.socket\framework.socket.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="lenovo.themes.generic">
      <HintPath>..\lib\lenovo.themes.generic.dll</HintPath>
    </Reference>
    <Reference Include="WindowsBase" />
  </ItemGroup>
	<Target Name="PostBuild" AfterTargets="PostBuildEvent">
		<Exec Command="copy $(OutDir)*.* $(SolutionDir)application" />
	</Target>
</Project>